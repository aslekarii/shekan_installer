{
    "server": "[::]:8880",
    "users": {
        "$UUID": "$PASSWORD"
    },
    "certificate": "/root/cert/cert.crt",
    "private_key": "/root/cert/private.key",
    "congestion_control": "bbr",
    "alpn": [
        "h3",
        "spdy/3.1"
    ],
    "udp_relay_ipv6": true,
    "zero_rtt_handshake": false,
    "dual_stack": true,
    "auth_timeout": "3s",
    "task_negotiation_timeout": "3s",
    "max_idle_time": "10s",
    "max_external_packet_size": 1500,
    "send_window": 16777216,
    "receive_window": 8388608,
    "gc_interval": "3s",
    "gc_lifetime": "15s",
    "log_level": "warn"
}